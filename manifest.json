{
  "name": "Keep Notes",
  "version": "0.0.1",
  "description": "Keep Notes of Every Tab",
  "permissions": ["activeTab", "tabs", "browsingData", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "logo.png"
  },
  "author": "IM",
  "action": {
    "default_popup": "index.html",
    "default_title": "Keep Notes",
    "default_icon": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["copy.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "manifest_version": 3
}
